{
  "name": "code-stats",
  "publisher": "my-extension",
  "type": "commonjs",
  "main": "./dist/extension.js",
  "displayName": "code-stats",
  "description": "This is the README for your extension \"code-stats\". After writing up a brief description, we recommend including the following sections.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/cassandra-driver": "^4.1.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.30",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "electron": "^35.2.0",
    "eslint": "^9.13.0",
    "playwright": "^1.52.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "statsDashboard",
          "title": "Stats Dashboard",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "statsDashboard": [
        {
          "id": "code-stats.webView",
          "type": "webview",
          "name": "Extension window",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "code-stats.viewDashboard",
        "title": "View Dashboard",
        "category": "Dashboard"
      },
      {
        "command": "code-stats.refreshDashboard",
        "title": "Refresh dashboard"
      },
      {
        "command": "code-stats.displaySidebar",
        "title": "Display sidebar"
      },
      {
        "command": "code-stats.authLogin",
        "title": "Login with Auth0"
      },
      {
        "command": "code-stats.showLogin",
        "title": "Show Login"
      },
      {
        "command": "code-stats.startAuthentication",
        "title": "Start Authentication"
      },
      {
        "command": "custom.pasteWithMessage",
        "title": "Paste with Message"
      },
      {
        "command": "custom.trackCopy",
        "title": "Track Copy"
      },
      {
        "command": "custom.trackCut",
        "title": "Track Cut"
      }
    ],
    "keybindings": [
      {
        "command": "custom.pasteWithMessage",
        "key": "cmd+v",
        "when": "editorTextFocus"
      },
      {
        "command": "custom.trackCopy",
        "key": "cmd+c",
        "when": "editorTextFocus"
      },
      {
        "command": "custom.trackCut",
        "key": "cmd+x",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc watch -p ./",
    "debug-shell": "tsc && node dist/debug-shell.js"
  },
  "dependencies": {
    "@snowplow/node-tracker": "^4.3.1",
    "@swdotcom/editor-flow": "^1.1.3",
    "axios": "^1.7.9",
    "body-parser": "^1.20.3",
    "cassandra-driver": "^4.7.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "node-cache": "^5.1.2",
    "swdc-tracker": "^1.5.7",
    "uuid": "^11.1.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
